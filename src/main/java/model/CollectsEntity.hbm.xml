<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.CollectsEntity" table="collects" schema="dbo" catalog="glasses">
        <composite-id mapped="true" class="model.CollectsEntityPK">
            <key-property name="customerId" column="customer_id"/>
            <key-property name="frameId" column="frame_id"/>
        </composite-id>
    </class>
</hibernate-mapping>