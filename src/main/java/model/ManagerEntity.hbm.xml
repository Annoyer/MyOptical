<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.ManagerEntity" table="manager" schema="dbo" catalog="glasses">
        <id name="managerId" column="manager_id"/>
        <property name="managerName" column="manager_name"/>
        <property name="managerPassword" column="manager_password"/>
    </class>
</hibernate-mapping>