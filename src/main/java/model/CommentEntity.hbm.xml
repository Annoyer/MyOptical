<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.CommentEntity" table="comment" schema="dbo" catalog="glasses">
        <id name="commId" column="comm_id"/>
        <property name="frameId" column="frame_id"/>
        <property name="commText" column="comm_text"/>
        <property name="commPhoto" column="comm_photo"/>
        <property name="customerId" column="customer_id"/>
        <property name="commTime" column="comm_time"/>
    </class>
</hibernate-mapping>