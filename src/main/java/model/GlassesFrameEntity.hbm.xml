<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.GlassesFrameEntity" table="glasses_frame" schema="dbo" catalog="glasses_shop">
        <composite-id mapped="true" class="model.GlassesFrameEntityPK">
            <key-property name="frameId" column="frame_id"/>
            <key-property name="glassesItemId" column="glasses_item_id"/>
        </composite-id>
    </class>
</hibernate-mapping>