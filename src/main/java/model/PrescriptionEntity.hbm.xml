<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.PrescriptionEntity" table="prescription" schema="dbo" catalog="glasses_shop">
        <composite-id mapped="true" class="model.PrescriptionEntityPK">
            <key-property name="presId" column="pres_id"/>
            <key-property name="userId" column="user_id"/>
        </composite-id>
        <property name="presDate" column="pres_date"/>
        <property name="presName" column="pres_name"/>
        <property name="rSph" column="r_sph"/>
        <property name="lSph" column="l_sph"/>
        <property name="rCyl" column="r_cyl"/>
        <property name="lCyl" column="l_cyl"/>
        <property name="rAxis" column="r_axis"/>
        <property name="lAxis" column="l_axis"/>
        <property name="pd" column="pd"/>
    </class>
</hibernate-mapping>